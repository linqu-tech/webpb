{
  "name": "webpb-webapp",
  "version": "0.0.1",
  "description": "A webpb sample webapp.",
  "scripts": {
    "build": "npm run proto && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "check": "npm run proto && npm run lint && npm run test",
    "dev": "webpack serve --config webpack/webpack.config.dev.js",
    "lint": "npm run lint:styles; npm run lint:scripts",
    "lint:scripts": "eslint --ext .ts --ext .js src",
    "lint:styles": "stylelint src",
    "proto": "run-script-os",
    "proto:default": "../../gradlew generateProto",
    "proto:win32": "../../gradlew.bat generateProto",
    "start": "npm run proto && npm run dev",
    "test": "jest --forceExit --coverage --silent --detectOpenHandles"
  },
  "license": "Apache-2.0",
  "keywords": [
    "webpb",
    "sample",
    "webapp"
  ],
  "devDependencies": {
    "@babel/core": "~7.15.0",
    "@babel/plugin-proposal-class-properties": "~7.14.5",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/preset-env": "~7.15.0",
    "@types/axios": "~0.14.0",
    "@types/jest": "~26.0.24",
    "@typescript-eslint/eslint-plugin": "~4.29.1",
    "@typescript-eslint/parser": "~4.29.1",
    "babel-loader": "~8.2.2",
    "clean-webpack-plugin": "~3.0.0",
    "cross-env": "~7.0.3",
    "css-loader": "~6.2.0",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "~2.24.0",
    "eslint-plugin-import-quotes": "~0.0.1",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prettier": "~3.4.0",
    "file-loader": "~6.2.0",
    "html-loader": "~2.1.2",
    "html-webpack-plugin": "~5.3.2",
    "jest": "~27.0.6",
    "jsdom": "~16.7.0",
    "mini-css-extract-plugin": "~2.2.0",
    "node-sass": "~6.0.1",
    "postcss-loader": "~6.1.1",
    "prettier": "~2.3.2",
    "run-script-os": "~1.1.6",
    "sass-loader": "~12.1.0",
    "style-loader": "~3.2.1",
    "stylelint": "~13.13.1",
    "stylelint-config-standard": "~22.0.0",
    "stylelint-webpack-plugin": "~3.0.1",
    "ts-jest": "~27.0.4",
    "ts-loader": "~9.2.5",
    "typescript": "~4.3.5",
    "webpack": "~5.49.0",
    "webpack-cli": "~4.7.2",
    "webpack-dev-server": "~3.11.2",
    "webpack-merge": "~5.8.0",
    "webpb": "~0.0.15"
  },
  "dependencies": {
    "@babel/polyfill": "~7.12.1",
    "axios": "~0.21.1",
    "core-js": "~3.16.1"
  }
}
