{
  "name": "webpb-webapp",
  "version": "0.0.1",
  "description": "A webpb sample webapp.",
  "scripts": {
    "build": "npm run proto && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "check": "npm run proto && npm run lint && npm run test",
    "dev": "webpack serve --config webpack/webpack.config.dev.js",
    "lint": "npm run lint:styles; npm run lint:scripts",
    "lint:scripts": "eslint --ext .ts --ext .js src",
    "lint:styles": "stylelint src",
    "proto": "run-script-os",
    "proto:default": "../../gradlew generateProto",
    "proto:win32": "../../gradlew.bat generateProto",
    "start": "npm run proto && npm run dev",
    "test": "jest --forceExit --coverage --silent --detectOpenHandles"
  },
  "license": "Apache-2.0",
  "keywords": [
    "webpb",
    "sample",
    "webapp"
  ],
  "devDependencies": {
    "@babel/core": "~7.16.0",
    "@babel/plugin-proposal-class-properties": "~7.16.0",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/preset-env": "~7.16.0",
    "@types/axios": "~0.14.0",
    "@types/jest": "~27.0.2",
    "@types/jsdom": "~16.2.13",
    "@typescript-eslint/eslint-plugin": "~5.3.1",
    "@typescript-eslint/parser": "~5.3.1",
    "babel-loader": "~8.2.3",
    "clean-webpack-plugin": "~4.0.0",
    "cross-env": "~7.0.3",
    "css-loader": "~6.5.1",
    "eslint": "~8.2.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "~2.25.3",
    "eslint-plugin-import-quotes": "~0.0.1",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prettier": "~4.0.0",
    "file-loader": "~6.2.0",
    "html-loader": "~3.0.1",
    "html-webpack-plugin": "~5.5.0",
    "jest": "~27.3.1",
    "jsdom": "~18.1.0",
    "mini-css-extract-plugin": "~2.4.4",
    "node-sass": "~6.0.1",
    "postcss-loader": "~6.2.0",
    "prettier": "~2.4.1",
    "run-script-os": "~1.1.6",
    "sass-loader": "~12.3.0",
    "style-loader": "~3.3.1",
    "stylelint": "~14.0.1",
    "stylelint-config-standard": "~23.0.0",
    "stylelint-webpack-plugin": "~3.1.0",
    "ts-jest": "~27.0.7",
    "ts-loader": "~9.2.6",
    "typescript": "~4.4.4",
    "webpack": "~5.64.0",
    "webpack-cli": "~4.9.1",
    "webpack-dev-server": "~4.5.0",
    "webpack-merge": "~5.8.0",
    "webpb": "~0.0.15"
  },
  "dependencies": {
    "@babel/polyfill": "~7.12.1",
    "axios": "~0.24.0",
    "core-js": "~3.19.1"
  }
}
