{
  "name": "webpb",
  "version": "0.0.15",
  "description": "The webpb runtime library for javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && cp README.md ./dist && cp package.json ./dist",
    "check": "npm run lint && npm run test",
    "lint": "eslint --quiet src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "deploy": "npm run check && npm run build && npm publish ./dist --access public",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles"
  },
  "author": "156023966@qq.com",
  "license": "Apache-2.0",
  "keywords": [
    "webpb",
    "runtime"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/linqu-tech/webpb"
  },
  "devDependencies": {
    "@types/jest": "~26.0.23",
    "@types/node": "~15.12.4",
    "@typescript-eslint/eslint-plugin": "~4.28.0",
    "@typescript-eslint/parser": "~4.28.0",
    "eslint": "~7.29.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "~2.23.4",
    "eslint-plugin-import-quotes": "~0.0.1",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prettier": "~3.4.0",
    "jest": "~27.0.5",
    "prettier": "~2.3.1",
    "pretty-quick": "~3.1.1",
    "ts-jest": "~27.0.3",
    "ts-node": "~10.0.0",
    "typescript": "~4.3.4"
  }
}
