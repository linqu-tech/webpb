{
  "name": "webpb",
  "version": "0.0.15",
  "description": "The webpb runtime library for javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && cp README.md ./dist && cp package.json ./dist",
    "check": "npm run lint && npm run test",
    "lint": "eslint --quiet src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "deploy": "npm run check && npm run build && npm publish ./dist --access public",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles"
  },
  "author": "156023966@qq.com",
  "license": "Apache-2.0",
  "keywords": [
    "webpb",
    "runtime"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/linqu-tech/webpb"
  },
  "devDependencies": {
    "@types/jest": "~27.4.0",
    "@types/node": "~17.0.8",
    "@typescript-eslint/eslint-plugin": "~5.9.1",
    "@typescript-eslint/parser": "~5.9.1",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-import-quotes": "~0.0.1",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prettier": "~4.0.0",
    "jest": "~27.4.7",
    "prettier": "~2.5.1",
    "pretty-quick": "~3.1.3",
    "ts-jest": "~27.1.3",
    "ts-node": "~10.4.0",
    "typescript": "~4.5.4"
  }
}
